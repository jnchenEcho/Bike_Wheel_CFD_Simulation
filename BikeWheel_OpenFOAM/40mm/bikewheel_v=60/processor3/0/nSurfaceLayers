FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      nSurfaceLayers;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            fixedValue;
        value           uniform 0;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 
0
;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    lowerWall
    {
        type            fixedValue;
        value           uniform 1;
    }
    upperWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    BikeWheel
    {
        type            fixedValue;
        value           nonuniform List<scalar> 
0
;
    }
    procBoundary3to5
    {
        type            processor;
        value           uniform 0;
    }
}


